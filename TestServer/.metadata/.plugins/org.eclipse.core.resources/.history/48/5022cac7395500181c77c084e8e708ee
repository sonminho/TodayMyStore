package controller;

public class ActionFactory {
	// 싱글턴 디자인
	private static ActionFactory instance = new ActionFactory();
	
	ActionFactory() {
		super();
	}
	
	public static ActionFactory getInstance() {
		return instance;
	}
	
	public Action getAction(String command) {
		Action action = null;
		
		System.out.println("ActionFactory : " + command);
	}
}
