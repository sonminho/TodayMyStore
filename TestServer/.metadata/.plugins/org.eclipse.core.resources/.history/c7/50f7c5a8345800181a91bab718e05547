package db;

import java.sql.Connection;

public class DBManager {
	public static Connection getConnection() {
		Connection conn = null;
		
		try {
			String dbURL = "jdbc:mysql://localhost:3306/";
			String dbId = "";
			String dbPw = "";
			Class.forName("com.mysql.jdbc.Driver");
			
		} catch(Exception e) {
			e.printStackTrace();
		}
		
		return conn;
	}
	
}
